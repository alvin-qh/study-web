{
  "name": "reactjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "clean": "rm -rf build storybook-static styleguide",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "storybook": "sb dev -p 9001 -c .storybook",
    "storybook:build": "sb build -c .storybook",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "analyze": "source-map-explorer build/static/js/main.*"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "~components/(.*)": "<rootDir>/src/components/$1"
    }
  },
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@blueprintjs/core": "^5.2.0",
    "@blueprintjs/icons": "^5.1.5",
    "@blueprintjs/select": "^5.0.8",
    "@blueprintjs/table": "^5.0.8",
    "@reduxjs/toolkit": "^1.9.5",
    "@storybook/react": "^7.3.2",
    "@testing-library/jest-dom": "^6.0.1",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.3",
    "@types/lodash": "^4.14.197",
    "@types/node": "^20.5.2",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/redux-immutable-state-invariant": "^2.1.2",
    "@types/redux-logger": "^3.0.9",
    "husky": "^8.0.3",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "prettier": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.15.0",
    "react-scripts": "5.1.0-next.14",
    "react-styleguidist": "^13.1.1",
    "redux": "^4.2.1",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.2",
    "source-map-explorer": "^2.5.3",
    "storybook": "^7.3.2",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "web-vitals": "^3.4.0"
  },
  "devDependencies": {
    "@redux-devtools/extension": "^3.2.5"
  }
}
