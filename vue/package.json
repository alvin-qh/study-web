    "moment": "^2.22.2",
    "noty": "^3.2.0-beta",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "page": "^1.8.6",
    "sinon": "^6.1.5",
    "sinon-chai": "^3.2.0",
    "style-loader": "^0.22.1",
    "typescript": "^3.0.1",
    "url-loader": "^1.1.1",
    "vue": "^2.5.17",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.3.0",
    "vue-notifications": "^0.9.0",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.17",
    "vuex": "^3.0.1",
    "webpack": "^4.16.5",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-cli": "^3.1.0",
