{
  "name": "vue-demo",
  "version": "1.0.0",
  "description": "Demo of vue and webpack",
  "keywords": [
    "vue",
    "webpack",
    "bootstrap"
  ],
  "scripts": {
    "setup": "npm install --update-binary",
    "package": "./node_modules/.bin/webpack --config webpack.config.babel.js",
    "package:prod": "NODE_ENV=production npm run package",
    "dev": "./node_modules/.bin/webpack-dev-server --inline"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.2.0",
    "@types/highlight.js": "^9.12.3",
    "ajv": "^6.5.3",
