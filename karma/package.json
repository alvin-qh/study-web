{
  "name": "karma-demo",
  "version": "1.0.0",
  "description": "karma test demo",
  "main": "karma.conf.js",
  "keywords": [
    "karma",
    "webpack",
    "mocha"
  ],
  "author": "",
  "license": "ISC",
  "scripts": {
    "setup": "npm install --update-binary",
    "test": "env NODE_ENV=test ./node_modules/.bin/karma start karma.conf.js || exit(0)",
    "test:cov": "env NODE_ENV=test ./node_modules/.bin/karma start karma-coverage.conf.js || exit(0)",
    "test:dev": "env NODE_ENV=test ./node_modules/.bin/karma start karma.conf.js --no-single-run --auto-watch || exit(0)"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.3",
    "@babel/runtime": "^7.8.4",
    "@fortawesome/fontawesome-free": "^5.12.1",
    "animate.css": "^3.7.2",
    "axios": "^0.19.2",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^5.0.2",
    "karma": "^4.4.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.1",
    "karma-jasmine": "^3.1.0",
    "karma-junit-reporter": "^2.0.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sinon-chai": "^2.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.2",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lodash": "^4.17.15",
    "mocha": "^7.0.1",
    "moment": "^2.24.0",
    "style-loader": "^1.1.3",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-cli": "^3.3.10"
  },
  "babel": {
    "presets": [
      "@babel/env"
    ],
    "plugins": [
      "@babel/transform-runtime"
    ]
  }
}
