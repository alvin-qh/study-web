{
  "name": "karma-demo",
  "version": "1.0.0",
  "description": "karma test demo",
  "main": "karma.conf.js",
  "keywords": [
    "karma",
    "webpack",
    "mocha"
  ],
  "author": "",
  "license": "ISC",
  "scripts": {
    "setup": "npm install --update-binary",
    "test": "NODE_ENV=test ./node_modules/.bin/karma start karma.conf.js",
    "test:cov": "NODE_ENV=test ./node_modules/.bin/karma start karma-coverage.conf.js",
    "test:dev": "NODE_ENV=test ./node_modules/.bin/karma start karma.conf.js --no-single-run --auto-watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5",
    "@babel/runtime": "^7.11.2",
    "@fortawesome/fontawesome-free": "^5.15.0",
    "animate.css": "^4.1.1",
    "axios": "^0.20.0",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "css-loader": "^4.3.0",
    "file-loader": "^6.1.0",
    "karma": "^5.2.3",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-junit-reporter": "^2.0.1",
    "karma-mocha": "^2.0.1",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sinon-chai": "^2.0.2",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-webpack": "^4.0.2",
    "less": "^3.12.2",
    "less-loader": "^7.0.1",
    "lodash": "^4.17.20",
    "mini-css-extract-plugin": "^0.11.2",
    "mocha": "^8.1.3",
    "moment": "^2.29.0",
    "webpack": "^4.44.2",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-cli": "^3.3.12"
  },
  "babel": {
    "presets": [
      "@babel/env"
    ],
    "plugins": [
      "@babel/transform-runtime"
    ]
  }
