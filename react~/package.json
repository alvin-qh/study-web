    "animate.css": "^4.1.1",
    "axios": "^0.20.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "bootstrap": "^4.5.2",
    "classnames": "^2.2.6",
    "core-js": "^3.6.5",
    "css-loader": "^4.3.0",
