{
  "name": "reactjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "eslint src --ext js,ts,jsx,tsx --fix",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "clean": "rm -rf build storybook-static styleguide",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "storybook": "sb dev -p 9001 -c .storybook",
    "storybook:build": "sb build -c .storybook",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "analyze": "source-map-explorer build/static/js/main.*"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "~components/(.*)": "<rootDir>/src/components/$1"
    }
  },
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@blueprintjs/core": "^5.7.2",
    "@blueprintjs/icons": "^5.5.0",
    "@blueprintjs/select": "^5.0.20",
    "@blueprintjs/table": "^5.0.20",
    "@reduxjs/toolkit": "^2.0.1",
    "@storybook/react": "^7.6.7",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@types/redux-immutable-state-invariant": "^2.1.4",
    "@types/redux-logger": "^3.0.12",
    "husky": "^8.0.3",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "prettier": "^3.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.21.1",
    "react-scripts": "5.1.0-next.14",
    "react-styleguidist": "^13.1.1",
    "redux": "^5.0.1",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^3.1.0",
    "source-map-explorer": "^2.5.3",
    "storybook": "^7.6.7",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "web-vitals": "^3.5.1"
  },
  "devDependencies": {
    "@redux-devtools/extension": "^3.3.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-simple-import-sort": "^10.0.0"
  }
}
