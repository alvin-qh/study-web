{
  "name": "react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@blueprintjs/core": "^3.49.1",
    "@blueprintjs/popover2": "^0.11.4",
    "@blueprintjs/select": "^3.18.1",
    "@blueprintjs/table": "^3.9.3",
    "@reduxjs/toolkit": "^1.6.1",
    "@types/lodash": "^4.14.172",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.1.18",
    "@types/react-router": "^5.1.16",
    "@types/react-router-dom": "^5.1.8",
    "@types/redux-devtools": "^3.7.0",
    "@types/redux-devtools-extension": "^2.13.2",
    "@types/redux-immutable-state-invariant": "^2.1.2",
    "@types/redux-logger": "^3.0.9",
    "dayjs": "^1.10.6",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
    "react-router-dom": "^5.3.0",
    "redux-devtools": "^3.7.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-logger": "^3.0.6",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@storybook/react": "^6.3.8",
    "@testing-library/dom": "^8.2.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "autoprefixer": "^9",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "compression-webpack-plugin": "^8.0.1",
    "customize-cra": "^1.0.0",
    "husky": "^7.0.2",
    "jest": "^27.1.0",
    "jest-watch-typeahead": "^0.6.4",
    "koa": "^2.13.1",
    "koa-router": "^10.1.1",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.12",
    "postcss": "^7",
    "postcss-fixes": "^2.0.1",
    "postcss-pxtorem": "^6",
    "prettier": "^2.3.2",
    "react-app-rewire-alias": "^1.0.2",
    "react-app-rewired": "^2.1.8",
    "react-scripts": "^4.0.3",
    "react-styleguidist": "^11.1.7",
    "source-map-explorer": "^2.5.2",
    "storybook": "^6.3.8",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "typescript": "^4.4.2",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.8.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "clean": "rm -rf build storybook-static styleguide",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "format": "prettier --single-quote --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook:build": "build-storybook -c .storybook",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "analyze": "source-map-explorer build/static/js/main.*",
    "api-server": "npx nodemon server/server.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "~components/(.*)": "<rootDir>/src/components/$1"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  }
}
